{
  "name": "Trello-report",
  "version": "1.0.0",
  "description": "a app that  helps Trello users to provide reports, filter cards, etc.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node server.js",
    "start-debug": "DEBUG='Trello-Report*' npm start",
    "start-watch": "DEBUG='abba*' ./node_modules/nodemon/bin/nodemon.js server.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test-debug": "DEBUG='abba*' ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "test:once": "karma start karma.conf.js --single-run",
    "lint:ts": "tslint ./src/**/*.ts -t verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raziyehbazargan/Trello-report.git"
  },
  "keywords": ['api','trello','oauth'],
  "author": "Raziyeh Bazargan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Raziyehbazargan/Trello-report/issues"
  },
  "homepage": "https://github.com/Raziyehbazargan/Trello-report#readme",
  "dependencies": {
    "aws-sdk": "^2.22.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "cowsay-browser": "^1.1.8",
    "debug": "^2.6.1",
    "del": "^2.2.2",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "express-session": "^1.15.2",
    "http-errors": "^1.6.1",
    "jsonwebtoken": "^7.3.0",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "node-sass": "^4.5.0",
    "nodemailer": "^4.0.0",
    "oauth": "^0.9.15",
    "path": "^0.12.7",
    "superagent": "^3.5.0",
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "~0.3.0",
    "systemjs": "0.19.40",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "concurrently": "^3.0.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.1.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "eslint": "^3.16.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "webpack": "^2.3.3",
    "canonical-path": "0.0.2",
     "tslint": "^3.15.1",
     "lodash": "^4.16.4",
     "jasmine-core": "~2.4.1",
     "karma": "^1.3.0",
     "karma-chrome-launcher": "^2.0.0",
     "karma-cli": "^1.0.1",
     "karma-jasmine": "^1.0.2",
     "karma-jasmine-html-reporter": "^0.2.2",
     "protractor": "~4.0.14",
     "rimraf": "^2.5.4",
     "@types/node": "^6.0.46",
     "@types/jasmine": "2.5.36"
  }
}
